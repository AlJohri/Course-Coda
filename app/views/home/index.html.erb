<div class="hero-unit" id="hero-main">
  <div class="tabbable"> <!-- Only required for left/right tabs -->
    <ul class="nav nav-tabs">
      <li class="tab-link active"><a href="#tab1" data-toggle="tab">Course</a></li>
      <li><a class="tab-link" href="#tab2" data-toggle="tab">Instruction</a></li>
      <li><a class="tab-link" href="#tab3" data-toggle="tab">Amount Learned</a></li>
      <li><a class="tab-link" href="#tab4" data-toggle="tab">Amount Stimulated</a></li>
      <li><a class="tab-link" href="#tab5" data-toggle="tab">Amount Challenged</a></li>
      <li><a class="tab-link" href="#tab6" data-toggle="tab">Feedback</a></li>
    </ul>
    <div class="tab-content">
      <div class="tab-pane active" id="tab1">
        <div class="data-chart" id="chart-course"></div>
      </div>
      <div class="tab-pane" id="tab2">
        <div class="data-chart" id="chart-instruction"></div>
      </div>
      <div class="tab-pane" id="tab3">
        <div class="data-chart" id="chart-learned"></div>
      </div>
      <div class="tab-pane" id="tab4">
        <div class="data-chart" id="chart-challenged"></div>
      </div>
      <div class="tab-pane" id="tab5">
        <div class="data-chart" id="chart-stimulated"></div>
      </div>
      <div class="tab-pane" id="tab6">
        <%= render :partial => 'feedback_tab', :locals => {:sections => @sections} %>
      </div>
    </div>
  </div>
</div>
